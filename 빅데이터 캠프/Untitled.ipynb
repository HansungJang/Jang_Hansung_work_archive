   "source": [
    "\"\"\"calculate income tax\"\"\" \n",
    "income = float(input(\"Enter the gross income: \"))\n",
    "dependents = int(input(\"Enter the number of dependents: \"))\n",
    "\n",
    "std_dec = 10000\n",
    "add_dec = 3000 * dependents\n",
    "tax = (0.2) * income - (std_dec + add_dec)\n",
    "\n",
    "print(\"The income tax is $%.2f\" % tax)"
   ]
  }
